                                        span(style= "color:rgb(130, 170, 255);") guard.
                                        span(style= "color:#dbd186;") handleError
                                        span(style="color: #c586c0;") ( )
                                        span.space
                                        span )
                                        span.space
                                        span ;
                            img(class='debug' src='/img/debugging.png')          



                    each ts in timeStamp
                        .timeStampStat(data-time = `${ts}`)
                    .error_overview_graph
                        .error_overview_graph_container
                            .past1dOverviewGraph 
                                .title Trends of Errors
                                    .timeline
                                        .value 24 hours
                                .inspect_container
                                    .inspect_text click to choose projects to inspect
                            #past1day(style="width: 100%;")
                            .dashboard_summary Summary 
                            .trends_summary_container 
                                .project_trends_title_container
                                    .project_trends_title Project 
                                    .project_trends_title_1 Total Errors Today
                                    .project_trends_title_2 Compare to last day
                                .project_trends_container 
                                each ts in past1dayErr
                                    .project_trends_i_container
                                        .project_trends_i_title #{ts[0]}
                                        .project_trends_i_total_container
                                            .project_trends_i_total #{ts[1].length}
                                        .project_trends_i_compare_container
                                            if ts[2] > 0
                                                .project_trends_i_compare_positive + #{ts[2]}
                                            else if ts[2] < 0
                                                .project_trends_i_compare_negative #{ts[2]}
                                            else
                                                .project_trends_i_compare_nochange ------
                    .project_container 
                        .project_container_header Project list
                        if projectsArr.length >= 1
                            .project_section_description 
                                .firstLine_container
                                    img.check(src='/img/green_check.png') 
                                    span.space
                                    span.space
                                    span.space
                                    .firstLine Your project is up and running! 
                                .secondLine If you have not seen any error information in your project, please make sure you have installed and initialize the SDK properly.
                        else 
                            .project_section_description 
                                .firstLine_container
                                    img.check(src='/img/light.png') 
                                    span.space
                                    span.space
                                    span.space
                                    .firstLine Create your first project to get started! 
                                .secondLine Start monitoring your apps using our error logging services, follow the instructions above to import and install our SDK.
                        .create_button_container                          
                            button#open-create-project + Create a Project
                        if projectsArr
                            .project_list_container
                                .project_list_title
                                    .project_title Project
                                    .project_user Users
                                    //- each project in projectsArr
                                    //-     .project_overview_info_container
                                    //-         a(class= 'project_name' href=`/a/${accountName}/prj/${project[0]}`) #{project[0]}
                                .project_list_project_container
                                    each project in projectsArr
                                        .project_overview_info_container  
                                            a(class= 'project_name' href=`/a/${accountName}/prj/${project[0]}`) #{project[0]}      
                                            a(class= 'project_user_numbers' href=`/a/${accountName}/${project[0]}/settings/members`) #{project[2]}  
                        

                    .create-project-window.hidden
                        .create-project-form-title
                            .form_title Let's Create a Project!
                            .form_description Error information in your NodeGuardian account is organized into Projects. Each project represents a single service or application.
                            .form_default 
                                .form_alert_setting_container
                                    img.check(src='/img/light.png') 
                                    .default_alert_setting.first A new project's default alert settings will send you a email whenever there is a new error.
                                        .default_alert_setting.second You can turn off this setting after creating a project.
                        .create-project-form
                            .value_contianer
                                .form_group
                                    label(for="projectName" class='form_projectName') PROJECT  NAME
                                    input#projectName(type="text" name="projectName" required)
                                .form_group
                                    label(for="token" class='form_token') PROJECT  TOKEN
                                    input#token(type="text" name="token" readonly)
                                .form_group.form_group_button
                                    span.space
                                    button#generate-token Generate Token
                                .form_button_group
                                    button#submit-create-project Create Project
                                    button.close_btn Cancel
                        


