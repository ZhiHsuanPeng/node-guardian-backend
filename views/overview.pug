doctype html 
html 
    head    
        
        meta(charset='UTF-8')
        meta(name='viewport' content='width=device-width, initial-scale=1.0')
        title Project lists
        link(rel='stylesheet' href='/css/style.css')
        script(src='https://cdn.plot.ly/plotly-2.32.0.min.js' charset="utf-8")
        link(href='https://cdn.lineicons.com/4.0/lineicons.css' rel='stylesheet')
        link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ' crossorigin='anonymous')
        link(rel='stylesheet' href='/css/style.css')
        link(rel="preconnect" href="https://fonts.googleapis.com")
        link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)
        link(href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet")
        link(rel="preconnect" href="https://fonts.googleapis.com")
        link(rel="preconnect" href="https://fonts.gstatic.com" crossorigin)
        link(href="https://fonts.googleapis.com/css2?family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet")

    body
        .wrapper
            aside#sidebar.expand
                .d-flex
                    button.toggle-btn(type='button')
                        i.lni.lni-grid-alt
                    .sidebar-logo
                        a(href='#') NodeGuardian
                ul.sidebar-nav
                    li.sidebar-item
                        a.sidebar-link(href='#')
                            i.lni.lni-user
                            span Profile
                    li.sidebar-item
                        a.sidebar-link(href=`/a/${accountName}`)
                            i.lni.lni-agenda
                            span Dashboard
                    li.sidebar-item
                        a.sidebar-link.collapsed.has-dropdown(href='#' data-bs-toggle='collapse' data-bs-target='#auth' aria-expanded='false' aria-controls='auth')
                            i.lni.lni-radio-button
                            span Projects
                        ul#auth.sidebar-dropdown.list-unstyled.collapse(data-bs-parent='#sidebar')
                            each project in projectsArr
                                li.sidebar-item
                                    a.sidebar-link(href=`/a/${accountName}/prj/${project[0]}`) #{project[0]} 
                    li.sidebar-item
                        a.sidebar-link.collapsed.has-dropdown(href='#' data-bs-toggle='collapse' data-bs-target='#auth' aria-expanded='false' aria-controls='auth')
                            i.lni.lni-cog
                            span Project Settings
                        ul#auth.sidebar-dropdown.list-unstyled.collapse(data-bs-parent='#sidebar')
                            each project in projectsArr
                                li.sidebar-item
                                    a.sidebar-link(href=`/a/${accountName}/${project[0]}/settings/general`) #{project[0]}
                    .sidebar-footer
                        a.sidebar-link.logout_btn(href='#')
                            i.lni.lni-exit
                            span Logout
            .main.p-3
                main.main
                    .welcome
                        .welcome_container
                            .text
                                .sub
                                    .welcome_text Welcome to NodeGuardian!
                                    .welcome_subtext Start catching and resolving errors fast.
                                .quick_start Install SDK from NPM
                                .SDK_setup_container
                                    .sdk_code
                                        span $
                                        span.space 
                                        span.space 
                                        span.space 
                                        span(style="color:rgb(130, 170, 255);") npm    
                                        span.space
                                        span(style="color:rgb(130, 170, 255);") install 
                                        span.space
                                        span --save
                                        span.space
                                        span.space
                                        span node-guardian
                                .quick_start Set Up NodeGuardian
                                .SDK_setup_container.init
                                    .sdk_code.comments
