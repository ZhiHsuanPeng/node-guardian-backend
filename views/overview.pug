                                        span.space
                                        span(style= "color:rgb(130, 170, 255);") accessToken: 
                                        span.space 
                                        span(style="color: #bb8a6b") "4719202ca6524e7fad65586"
                                        span.space
                                        span.space 
                                        span })
                                    .sdk_code.comments.two
                                        span //
                                        span.space 
                                        span.space 
                                        span.sapce 
                                        span use 
                                        span.sapce  
                                        span it 
                                        span.sapce 
                                        span in 
                                        span.sapce 
                                        span express 
                                        span.sapce 
                                        span framework 
                                        span.space
                                        span as 
                                        span.sapce 
                                        span a
                                        span.sapce  
                                        span middleware
                                    .sdk_code
                                        span(style= "color:rgb(130, 170, 255);") app
                                        span .use
                                        span.space
